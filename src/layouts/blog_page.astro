---
import Layout from "./posinega.astro";
import Navbar from "../components/navbar.astro";
import Footer from "../components/footer.astro";

const { title } = Astro.props
---

<Layout title=`${title}`>
	<Navbar slot="header" />
	<h1>{title}</h1>
	<slot />
	<Footer />
</Layout>

<style>
	h1 {
		font-size: 2rem;
	}
</style>
